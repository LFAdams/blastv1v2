#PBS -S /bin/bash
#PBS -N blasttest
#PBS -l nodes=1:ppn=4:AMD
#PBS -q batch
#PBS -l walltime=480:00:00
#PBS -l mem=20gb

cd /work/cemlab/reference_genomes/

module load BLAST+/2.7.1-foss-2016b-Python-2.7.14
blastn -num_threads 4 -db 97103_v2 -query /home/lfa81121/blastv1v2/nbsgenes.txt -out /home/lfa81121/nbs1v2 -outfmt 6
